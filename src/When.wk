every When{T} is:

	with T[] returns = [];
	with Num = 0;

	When{T} -- thenReturn(T) {
		returns.push(T);
		return this;
	}

	T -- get() {
		var T? = returns[?Num];
		if T exists {
			Num += 1;
			return T;
		} else {
			Num = 0.orMaxOf(Num - 1); // go back one, stopping at 0
			return returns[Num]; // will throw when we stopped at 0
		}
	}
