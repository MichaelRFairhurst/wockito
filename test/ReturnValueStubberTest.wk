import Asserts;
import ReturnValueStubber;

every ReturnValueStubberTest is:

	needs ReturnValueStubber{Num};

	~[ test .thenReturn() on an argument stubber, and then getting its return value ]~(Asserts) {
		ReturnValueStubber.thenReturn(1).thenReturn(2).thenReturn(3);

		Asserts.that(ReturnValueStubber.get())Equals(1);
		Asserts.that(ReturnValueStubber.get())Equals(2);
		Asserts.that(ReturnValueStubber.get())Equals(3);
	}
